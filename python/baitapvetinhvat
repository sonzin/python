def calAverage(no1,no2,no3,no4):
    total = no1 + no2 + no3 + no4
    average = total / 4
    print(average)
    return average

no1=int(input('Put your number: '))
no2=int(input('Put your number: '))
no3=int(input('Put your number: '))
no4=int(input('Put your number: '))
calAverage(no1,no2,no3,no4)
calAverage(2,43,12,76)
